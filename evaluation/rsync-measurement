#!/bin/sh

if [ ! $# -eq 1 ] ; then
   echo >&2 "Usage: $0 measurement_name"
   exit 1
fi

if [ ! -d $1 ] ; then
   echo >&2 "ERROR: $1 does not exist!"
   exit 1
fi

rsync -Pavz Run-$1 plot-$1.R Results-$1-active.pdf Results-$1-passive.pdf dominos.simula.no:/var/foivos/OAI/
rsync -Pavz $1 dominos.simula.no:/var/foivos/OAI/
